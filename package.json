{
  "name": "crdb-config-explorer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "classnames": "^2.2.6",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "typescript": "^5.3.0"
  },
  "scripts": {
    "start": "esbuild src/index.tsx --bundle --outdir=public/build --servedir=public --serve=3000 --watch",
    "build": "rm -rf build && mkdir -p build && cp -r public/* build/ && esbuild src/index.tsx --bundle --outdir=build/build --minify --sourcemap",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "prettier": "^3.2.0"
  }
}
